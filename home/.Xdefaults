URxvt.background: [75]#2c2c2c

!! Hard-coded foreground color overrides some GUI apps.
!! It's useful for urxvt, but not at the cost of other apps. 
!! Leaving commented out for now.
!! *foreground: #dcdccc
!! *cursorColor: #93a1a1

!! black dark/light
*color0: #000000
*color8: #686868

!! red dark/light
*color1: #B21818
*color9: #FF5454

!! green dark/light
*color2: #18B218
*color10: #54FF54

!! yellow dark/light
*color3: #B26818
*color11: #FFFF54

!! blue dark/light
*color4: #1818B2
*color12: #5454FF

!! magenta dark/light
*color5: #B218B2
*color13: #FF54FF

!! cyan dark/light
*color6: #18B2B2
*color14: #54FFFF

!! white dark/light
*color7:  #B2B2B2
*color15: #FFFFFF

URxvt*depth: 32
URxvt*saveLines: 1000
URxvt*font: xft:Monospace:pixelsize=14:antialias=true:hinting=true
URxvt*boldFont: xft:Monospace:bold:pixelsize=14:antialias=true:hinting=true

!! urxvt*background: rgba:0000/0000/0200/c800
URxvt.transparent:   true
URxvt.shading:       40

URxvt.perl-ext-common: default,matcher,clipboard
URxvt.keysym.C-S-c:   perl:clipboard:copy
URxvt.keysym.C-S-v:   perl:clipboard:paste
URxvt.keysym.C-S-C-v: perl:clipboard:paste_escaped
URxvt.url-launcher: /usr/bin/firefox -new-tab
URxvt.matcher.button: 1
URxvt.colorUL: #4682B4
URxvt.clipboard.autocopy: true

!! Tighten up font
URxvt.letterSpace: -1

URxvt*scrollBar: false
URxvt*scrollBar_right: false
URxvt*scrollstyle: rxvt

visualbell: on
